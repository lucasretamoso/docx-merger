{
  "name": "docx-merger",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Javascript Library for Merging Docx file in NodeJS and Browser Environment.",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "echo \"Error: no test specified\" && exit 0",
    "validate": "echo \"Check For Test Coverage\" && exit 0",
    "prebuild": "rimraf dist",
    "build": "esbuild ./src/index.js --bundle --platform=node --packages=external --target=es2020 --outfile=dist/index.js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apurvaojas/docx-merger.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "docx",
    "composition",
    "document",
    "office",
    "docx merge",
    "microsoft word"
  ],
  "author": "Apurv Ojas <apurvaojas10@gmail.com>",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.6",
    "jszip": "^2.6.1"
  },
  "bugs": {
    "url": "https://github.com/apurvaojas/docx-merger/issues"
  },
  "homepage": "https://github.com/apurvaojas/docx-merger#readme",
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "esbuild": "^0.16.5",
    "ghooks": "^2.0.4",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.12.3",
    "travis-deploy-once": "^5.0.9"
  },
  "engines": {
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
